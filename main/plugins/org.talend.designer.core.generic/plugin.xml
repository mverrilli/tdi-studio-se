   <extension
         point="org.talend.designer.core.generic.fieldType_mapping">
      <Mapping
            mapFieldType="CHECK"
            name="widget.type.default+java.lang.Boolean"
            schemaType="java.lang.Boolean"
            widgetType="widget.type.default">
      </Mapping>
      <Mapping
            mapFieldType="CHECK"
            name="widget.type.default+java.util.List&lt;java.lang.Boolean&gt;"
            schemaType="List&lt;java.lang.Boolean&gt;"
            widgetType="CHECK">
      </Mapping>
      <Mapping
            mapFieldType="DATE"
            name="widget.type.default+java.util.Date"
            schemaType="java.util.Date"
            widgetType="widget.type.default">
      </Mapping>
      <Mapping
            mapFieldType="CLOSED_LIST"
            name="widget.type.enumeration"
            widgetType="widget.type.enumeration">
      </Mapping>
      <Mapping
            mapFieldType="SCHEMA_TYPE"
            name="widget.type.default+SCHEMA"
            schemaType="SCHEMA"
            widgetType="widget.type.default">
      </Mapping>
      
       <Mapping
            mapFieldType="BUTTON"
            name="widget.type.button"
            widgetType="widget.type.button">
      </Mapping>
       <Mapping
            mapFieldType="COMPONENT_REFERENCE"
