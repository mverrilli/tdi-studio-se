             widgetType="widget.type.textArea">
       </Mapping>
       <Mapping
             mapFieldType="JSON_TABLE"
             name="widget.type.jsonTable"
             widgetType="widget.type.jsonTable">
       </Mapping>
       <Mapping
             mapFieldType="DIRECTORY"
             name="widget.type.directory"
             widgetType="widget.type.directory">
       </Mapping>
       <Mapping
             mapFieldType="QUERYSTORE_TYPE"
             name="widget.type.querystoreType"
             widgetType="widget.type.querystoreType">
       </Mapping>
       <Mapping
             mapFieldType="MEMO_SQL"
