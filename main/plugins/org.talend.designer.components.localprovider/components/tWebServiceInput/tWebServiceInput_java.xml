	    <PARAMETER
	      NAME="MATCHBRACKETS"
	      FIELD="CLOSED_LIST"
	      SHOW_IF="ADVANCED_USE == 'true'"
	      REQUIRED="false"
	      NUM_ROW="60"
	    >
	      <ITEMS DEFAULT="NONE">
	      	<ITEM NAME="NONE"   VALUE="" />
	        <ITEM NAME="ONE"   VALUE="}" />
	        <ITEM NAME="TWO"   VALUE="}}" />
	        <ITEM NAME="THREE"    VALUE="}}}" />
	      </ITEMS>
